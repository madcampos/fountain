{
  "name": "fountain",
  "version": "0.1.0",
  "author": "madcampos",
  "description": "fountain-ionic: An Ionic project",
  "main": "dist/main.js",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "gulp build",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "start": "electron .",
    "electron": "electron .",
    "electron dist": "electron .",
    "clear:osx": "rm -rf *-darwin-x64; rm -rf ./platforms/*-mas-x64",
    "clear:win": "rm -rf *-win32-ia32; rm -rf ./platforms/*-win32-x64",
    "clear:nix": "rm -rf *-linux-ia32; rm -rf ./platforms/*-linux-x64",
    "pack:osx": "npm run clear:osx && electron-packager ./www/ --platform=darwin,mas --arch=x64 --asar --out=platforms ",
    "pack:win": "npm run clear:win && electron-packager ./www/ --platform=win32 --arch=ia32,x64 --asar --out=platforms ",
    "pack:nix": "npm run clear:nix && electron-packager ./www/ --platform=linux --arch=ia32,x64 --asar --out=platforms "
  },
  "dependencies": {
    "@angular/common": "4.0.2",
    "@angular/compiler": "4.0.2",
    "@angular/compiler-cli": "4.0.2",
    "@angular/core": "4.0.2",
    "@angular/forms": "4.0.2",
    "@angular/http": "4.0.2",
    "@angular/platform-browser": "4.0.2",
    "@angular/platform-browser-dynamic": "4.0.2",
    "@ionic/storage": "2.0.1",
    "font-manager": "^0.2.2",
    "fontkit": "^1.5.4",
    "installfont": "0.0.9",
    "ionic-angular": "3.1.0",
    "ionicons": "3.0.0",
    "opentype": "^0.1.2",
    "rxjs": "5.1.1",
    "sw-toolbox": "3.6.0",
    "zone.js": "^0.8.9"
  },
  "devDependencies": {
    "@ionic/app-scripts": "1.3.5",
    "del": "^2.2.2",
    "electron": "^1.6.6",
    "electron-package": "^0.1.0",
    "gulp": "^3.9.1",
    "gulp-electron": "^0.1.3",
    "gulp-run": "^1.7.1",
    "gulp-watch": "^4.3.11",
    "ionic-gulp-browserify-typescript": "^2.0.0",
    "ionic-gulp-fonts-copy": "^1.0.0",
    "ionic-gulp-html-copy": "^1.0.0",
    "ionic-gulp-sass-build": "^1.0.0",
    "ionic-gulp-scripts-copy": "^2.0.1",
    "run-sequence": "^1.2.2",
    "tslint": "^5.1.0",
    "tslint-eslint-rules": "^4.0.0",
    "typescript": "~2.2.2"
  }
}
